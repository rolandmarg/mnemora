{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Main App",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Delete All Events (Dry Run)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["delete-all-events"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Delete All Events (Confirm) - DESTRUCTIVE",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
